const hashMap = [
    {logo:'A', logoType: 'text', url:'https://www.acfun.cn'},
    {logo:'./image/test.png', url:'https://www.bilibili.com'}
]

hashMap.forEach(node=>{
    const $li = $(`<li>
    <a href="${node.url}">
        <div class="site">
            <div class="logo">${node.logo}</div>
            <div class="link">${node.url}</div>
        </div>
    </a>            
</li>`)
})

